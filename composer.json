{
  "name": "dnt/localization",
  "description": "Localization for laravel",
  "type": "library",
  "author": "DNT",
  "homepage": "https://github.com/ducconit/localization",
  "keywords": [
    "laravel location",
    "location",
    "localization",
    "route localization",
    "dnt",
    "ducconit"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/routing": "^8.55|^9.0|^10.0",
    "illuminate/support": "^8.55|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "DNT\\Localization\\": "src/"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true,
  "authors": [
    {
      "name": "ducnt",
      "email": "ducnt@sphoton.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "DNT\\Localization\\LocalizationServiceProvider"
      ]
    }
  }
}
